  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="sfAdminVersion" value="20170715.01" />
    <add key="sfSecureDomain" value="admin.iot-smartfactory.net" />
    <add key="sfAPIServiceBaseURI" value="http://xxxxxx/" />
    <add key="sfAPIServiceTokenRole" value="admin" />
    <add key="sfDocDBConnectionString" value="AccountEndpoint=https://xxxxxxxxxxxxxx" />
    <add key="sfServiceBusConnectionString" value="Endpoint=sb://xxxxxxxxxxx" />
    <add key="sfInfraOpsQueue" value="infraops" />
    <add key="sfProcessCommandTopic" value="processcommand" />

    <add key="sfLogStorageName" value="xxxxxx" />
    <add key="sfLogStorageKey" value="xxxxxx" />
    <add key="sfLogStorageContainerApp" value="log-admin-webapp" />
    <add key="sfLogStorageContainerAudit" value="log-audit-admin" />
    <add key="sfLogLevel" value="Error" />

    <add key="sfGoogleMapAPIKey" value="xxxxxx" />
    <add key="sfBaiduMapAPIKey" value="xxxxxx" />

    <add key="i18n.DirectoriesToScan" value=".." />
    <!-- Rel to web.config file -->
    <add key="i18n.WhiteList" value="*.cs;*.cshtml;*.sitemap;*.js" />
    <add key="i18n.BlackList" value=".\js\kendo;.\js\angular;.\*\dist" />

  </appSettings>

  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
    <httpModules>
      <add name="i18n.LocalizingModule" type="i18n.LocalizingModule, i18n" />
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
    </httpModules>

    <sessionState mode="Custom" customProvider="MySessionStateStore">
      <providers>
        <add name="MySessionStateStore" type="Microsoft.Web.Redis.RedisSessionStateProvider" host="xxxxxx" port="6380" accessKey="xxxxxx" ssl="true" throwOnError="true" />
      </providers>
    </sessionState>

  </system.web>
