      </bindingExtensions>
    </extensions>
  </system.serviceModel>
  <appSettings>
    <!-- Service Bus specific app setings for messaging connections -->    
    <add key="sfServiceBusConnectionString" value="Endpoint=sb://xxxxxxxxxx" />
    <add key="sfDocDBConnectionString" value="https://xxxxxx" />
    <add key="sfAdminHeartbeatURL" value="http://xxxxxxxxxxx/Monitor/RTMessageFeedIn" />
    <add key="sfSuperAdminHeartbeatURL" value="http://xxxxxxxxxxx/Monitor/RTMessageFeedIn" />
    <add key="RTMessageFeedInURL" value="http://xxxxxxxxxxx/Monitor/RTMessageFeedIn" />
    <add key="sfProcessCommandTopic" value="processcommand" />
    <add key="sfAlarmOpsQueue" value="alarmops" />
    <add key="sfInfraOpsQueue" value="infraops" />
    <add key="sfLogStorageName" value="xxxxxx" />
    <add key="sfLogStorageKey" value="xxxxxx" />
    <add key="sfLogStorageContainerApp" value="log-backend-iothubeventprocessor" />
    <add key="sfLogLevel" value="Info" />
  </appSettings>
  <connectionStrings>    
    <add name="SFDatabaseEntities" connectionString="metadata=res://*/SFDatabaseEntities.csdl|res://*/SFDatabaseEntities.ssdl|res://*/SFDatabaseEntities.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=xxxxxx.database.windows.net;initial catalog=xxxxxx;user id=kevika;password=xxxxxx;multipleactiveresultsets=True;application name=EntityFramework&quot;" providerName="System.Data.EntityClient" />    
  </connectionStrings>
